{
    "name": "kashima",
    "description": "A electron application to play Music within a directory.",
    "version": "0.0.1",
    "main": "src/index.js",
    "bugs": { "url": "https://github.com/auguwu/kashima/issues" },
    "dependencies": {
        "discord-rpc": "3.0.1",
        "music-metadata": "3.4.0"
    },
    "devDependencies": {
        "electron": "4.0.0",
        "electron-builder": "20.38.4"
    },
    "scripts": {
        "start": "electron .",
        "build": "electron-builder",
        "pack": "electron-builder --pack"
    },
    "author": "auguwu",
    "build": {
        "win": {
            "target": "nsis"
        },
        "linux": {
            "target": ["AppImage", "deb"]
        }
    }
}